{
  "private": true,
  "scripts": {
    "favicon-generate": "real-favicon generate faviconDescription.json build/faviconData.json dist",
    "favicon-inject": "real-favicon inject build/faviconData.json build src/wrapper.html",
    "favicon-check": "real-favicon check-for-update --fail-on-update build/faviconData.json",
    "html-minifier": "html-minifier --collapse-whitespace --remove-comments --use-short-doctype --minify-css true build/index.html -o dist/index.html",
    "marked": "marked -o build/content.html -i src/index.md --mangle",
    "test": "npm run favicon-check"
  },
  "dependencies": {
    "cli-real-favicon": "0.0.8",
    "html-minifier": "^4.0.0",
    "marked": "^1.2.2"
  }
}
